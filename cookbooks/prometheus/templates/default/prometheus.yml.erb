global:
  scrape_interval: 5s
  evaluation_interval: 1s

rule_files:
 - /rules/*.rules

alerting:
  alertmanagers:
    - static_configs:
      - targets:
        - chordata.sinkingpoint.com:9090

scrape_configs:
  - job_name: alertmanager
    static_configs:
      - targets:
        - chordata.sinkingpoint.com:9090
  - job_name: node_exporter
    static_configs:
      - targets:
        - chordata.sinkingpoint.com:9100
        - callabus.sinkingpoint.com:9100